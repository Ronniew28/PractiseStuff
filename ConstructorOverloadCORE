
package constructoroverloadcore;

/**
 *
 * @author Ronnie W
 */
public class ConstructorOverloadCORE {
    private int num1;
    private int num2;
    private String digit;
    
        public ConstructorOverloadCORE () {
            num1 = 7;
            num2 = 5;
            digit = "number";
        }
        
            public ConstructorOverloadCORE(int num1, int num2, String digit) {
                this.digit = digit;
                this.num1 = num1;
                this.num2 = num2;
            }
            
                   
    public static void main(String[] args) {
            ConstructorOverloadCORE N1 = new ConstructorOverloadCORE();
            System.out.println(N1.num1);
            
            ConstructorOverloadCORE N2 = new ConstructorOverloadCORE();
            System.out.println(N2.num2);
            
            ConstructorOverloadCORE N3 = new ConstructorOverloadCORE();
            System.out.println(N3.digit);
    }
    
}
