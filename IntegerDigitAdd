
package integerdigitadd;

import java.util.Scanner;

/**
 *
 * @author Ronnie W
 */
public class IntegerDigitAdd {

    
    public static void main(String[] args) {
     int num = 7777;
     int sum = 0;
     while (num > 0) {
         sum = sum + num % 10;
         num = num / 10;
     }
     System.out.println(sum);
        
    }
    
}
