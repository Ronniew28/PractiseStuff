
package accessdemo;

/**
 *
 * @author Ronnie W
 */
public class AccessDemo {
    private int alpha;
    public int beta;
    
    void setAlpha(int a) {
        alpha = a;
    }
    
    int getAlpha() {
        return alpha;
    }
    
    public static void main(String[] args) {
        AccessDemo ob = new AccessDemo();
        
        ob.setAlpha(-99);
        System.out.println("ob.alpha is " + ob.getAlpha());
    }
    
}
