
package constmultiple;

/**
 *
 * @author Ronnie W
 */
public class ConstMULTIPLE {
    
    int x;
    
    ConstMULTIPLE(int i) {
        x = i;
    }

    
    public static void main(String[] args) {
        ConstMULTIPLE add = new ConstMULTIPLE(7);
        ConstMULTIPLE add1 = new ConstMULTIPLE(5);
        
        System.out.println(add.x + add1.x);//12
        System.out.println(add.x - add1.x);//2
        System.out.println(add.x * add1.x);//35
        System.out.println(add.x / add1.x);//1
    }
    
}
