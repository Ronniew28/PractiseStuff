
package pkgwhile;

/**
 *
 * @author Ronnie W
 */
public class While {

    
    public static void main(String[] args) {
        int x = 0;
            while (x <=5) {
                System.out.println(x);
                x++;
            }
            
        int y = 5;
            while (y>=0) {
                System.out.println(y);
                y--;
            }
    }
    
}
