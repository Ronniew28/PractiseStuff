
package empty3;

/**
 *
 * @author Ronnie W
 */
public class Empty3 {

    
    public static void main(String[] args) {
        int i;
        int sum = 0;
        
        for(i = 1; i<=5; sum += i++); //empty statement after for
        
        System.out.println("Sum is " + sum);
        
        //sum +=i++ is the same as -->
        //sum = sum+1;
        //i++;
    }
    
}
