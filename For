
package pkgfor;

/**
 *
 * @author Ronnie W
 */
public class For {

    
    public static void main(String[] args) {
        for (int x = 1; x<=5; x++) {
            System.out.println(x);
        }
        
        for (int y = 1; y<=5; y++) {
            System.out.println(y);
        }
    }
    
}
