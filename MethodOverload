
package methodoverloadwork;

/**
 *
 * @author Ronnie W
 */
public class MethodOverloadWork {

    
    public static void main(String[] args) {
        System.out.println(Add(2, 3, 4));
        System.out.println(Minus(25, 20));
        System.out.println(Multiply(7, 7));
        System.out.println(Divide(25, 5));
    }
    
    public static int Add(int a , int b , int c) {
        return (a+b+c);
    }
    public static int Minus(int d , int e) {
        return(d-e);
        }
    public static int Multiply(int f , int g) {
        return(f*g);
    }
    public static int Divide(int h , int i) {
        return(h/i);
    }
    
}
